    "1b23f5f7-b2a4-4c4c-8e32-6d537f1aa393": {
      "type": "image-with-text",
      "blocks": {
        "template--16719143305468__1b23f5f7-b2a4-4c4c-8e32-6d537f1aa393-168369264817ddd21f-0": {
          "type": "heading",
